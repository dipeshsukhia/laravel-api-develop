{
    "name": "dipeshsukhia/laravel-api-develop",
    "description": "Develop rest APIs for Laravel",
    "keywords": [
        "dipeshsukhia",
        "laravel-api-develop"
    ],
    "homepage": "https://github.com/dipeshsukhia/laravel-api-develop",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Dipesh Sukhia",
            "email": "dipesh.sukhia@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Dipeshsukhia\\LaravelApiDevelop\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dipeshsukhia\\LaravelApiDevelop\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dipeshsukhia\\LaravelApiDevelop\\LaravelApiDevelopServiceProvider"
            ],
            "aliases": {
                "LaravelApiDevelop": "Dipeshsukhia\\LaravelApiDevelop\\LaravelApiDevelopFacade"
            }
        }
    }
}
